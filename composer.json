{
    "name": "jaddek/kraken-http-client",
    "type": "library",
    "authors": [
        {
            "name": "tonynazarov",
            "email": "tonynazarov@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "symfony/http-client": "^6.2",
        "ext-random": "*"
    },
    "autoload": {
        "psr-4": {
            "Jaddek\\Kraken\\Http\\Client\\": "src/"
        }
    },
    "require-dev": {
        "vimeo/psalm": "^5.5",
        "squizlabs/php_codesniffer": "*",
        "slevomat/coding-standard": "^8.8"
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
